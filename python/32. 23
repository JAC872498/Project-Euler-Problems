"""

def pan(num):
	for n in range(len(str(num))):
		if(str(n+1) not in str(num)):
			return(False)
	return(True)
def formsPan(n1,n2,prevNums):
	if(pan(str(n1)+str(n2)+str(n1*n2)) and str(n1*n2) not in prevNums):
		return(True)
	return(False)
	
total=0
n2=1
prevNums=[]
for n1 in range(1,9877):
	while(len(str(n1)+str(n2)+str(n1*n2))<=9):
		if(pan(str(n1)+str(n2)+str(n1*n2)) and str(n1*n2) not in prevNums):
			prevNums.append(str(n1*n2))
			total+=(n1*n2)
	n2+=1
print(total)
