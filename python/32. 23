"""
"""

def pan(num):
	for n in range(1,10):
		if(str(n) not in str(num)):
			return(False)
	return(True)
total=0
prevNums=[]
for n1 in range(1,9877):
	n2=1
	while(len(str(n1)+str(n2)+str(n1*n2))<=9):
		if(pan(str(n1)+str(n2)+str(n1*n2)) and str(n1*n2) not in prevNums):
			prevNums.append(str(n1*n2))
			total+=(n1*n2)
		n2+=1
print(total)
