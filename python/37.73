def isPrime(num):
	for x in range(num-2):
		if(num%(x+2)==0):
			return False
	return True

def trun(num):
	word=str(num)
	for n in range(len(word)):
		if(not isPrime(int(word[n:])) or not isPrime(int(word[:n+1]))):
			return False
	return True

count=0
tot=0
curr=9
while(True):
	curr+=2
	if(trun(curr)):
		tot+=curr
		count+=1
	if(count==11):
		break
print(tot)
