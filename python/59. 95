key=[0,0,0]
"""print(bin(65))
print(bin(42))
print(65^42)
print(107^42)
print(chr(0))
print(chr(255))"""

ciphered=[]
cipher=open("cipher.txt", "r")
ciphered=(cipher.read().split(","))
cipher.close()

normal=[]
for n in range(len(ciphered)):
	#print(chr(int(ciphered[n])^key[0]))
	normal.append(chr(int(ciphered[n])^key[n%len(key)]))

print("".join(normal))
