def prime(num):
	for x in range(num-2):
		if(num%(x+2)==0): return False
	return True

def endless(num):
	length=0
	for x in range(num):
		if(prime(x+1)and(x+1!=2 or x+1!=5)):
			return True
	return False
	#return length
#Num repeates if denominator has prime factor other than 2 or 5 
