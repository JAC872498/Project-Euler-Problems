from decimal import *
getcontex().prec=28
def prime(num):
	for x in range(num-2):
		if(num%(x+2)==0): return False
	return True

def endless(num):
	list=[]
	for x in range(num-1):
		if(num%(x+2)==0 and prime(x+2)): list.append(x+2)
	for n in list:
		if(n!=2 and n!=5):
			return True
	return False
	
def rep(num):
	starting=1
	while(True):
		starting+=1
		length=0
		while(length<=len(str(num))-1):
			length+=1
			wrd=str(num)[starting:length+starting]
			if(str(num)[starting:length+starting]==str(num)[starting+length:(2*length)+starting]):
				return length
		
top=0
longest=0
for x in range(9):
	if(endless(x+2)):
		print(Decimal(rep(1.0/(x+2))),x+2)
		if(Decimal(rep(1.0/(x+2)))>longest):
			longest=Decimal(rep(1.0/(x+2)))
			top=x+2
print(top," is the longest with a chain of ", longest)
print Decimal((1.0/7)))
print 1.0/7
print Decimal((1.0/81))
print 1.0/81
