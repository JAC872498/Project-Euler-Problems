""""""

def prime(num):
	for x in range(num-2):
		if(num%(x+2)==0): return False
	return True

def endless(num):
	list=[]
	for x in range(num-1):
		if(num%(x+2)==0 and prime(x+2)): list.append(x+2)
	for n in list:
		if(n!=2 and n!=5):
			return True
	return False
	
def rep(num):
	starting=-1
	while(True):
		starting+=1
		length=0
		while(length<=len(str(num))-1):
			length+=1
			wrd=string(num)[starting:length+starting]
			if(#wrd is the repeting):
				return length
	return length
	
d=1#the repeting decimal part can't be longer than d-1
top=6
biggest=1
while(d<1000):
	d+=1
	if(endless(d) and rep(1/d))>=top):
		top=endless(1/d)
		biggest=d
