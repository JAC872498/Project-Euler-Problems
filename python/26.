"""
"""

def prime(num):
	for x in range(num-2):
		if(num%(x+2)==0): return False
	return True
	
def rep(n, d):
    x = n * 9
    z = x
    k = 1
    while z % d:
        z = z * 10 + x
        k += 1
    return k
	
longest=7
chain=6
for x in range(10,1001):
	if(prime(x)):
		if(rep(1,x)>chain):
			longest=x
			chain=rep(1,x)
	
print(longest," is the longest with a chain of ",chain)
