d=0
top=0
best=0
for x in range(1000):
	d+=1
	wrd=1.0/d
	starting=0
	length=1
	while(str(wrd)[starting:length+starting]!=str(wrd)[starting+length:(length*2)+starting]):
		length+=1
		if(length>d-1):
			length=1
			starting+=1
	if(length>best):
		best=length
		top=d
print(d)
