""""""

def prime(num):
	for x in range(num-2):
		if(num%(x+2)==0): return False
	return True

def endless(num):
	list=[]
	for x in range(num-1):
		if(num%(x+2)==0 and prime(x+2)): list.append(x+2)
	for n in list:
		if(n!=2 and n!=5):
			return True
	return False
	
def rep(d):
	wrd=1.0/d
	starting=0
	length=1
	while(str(wrd)[starting:length+starting]!=str(wrd)[starting+length:(length*2)+starting]):
		length+=1
		if(length>d-1):
			length=1
			starting+=1
	print(length)
	
d=1#the repeting decimal part can't be longer than d-1
top=6
biggest=1
while(d<1000):
	d+=1
	if(endless(d) and rep(1/d)>=top):
		top=endless(1/d)
		biggest=d
print(d)
