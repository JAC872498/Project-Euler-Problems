def prime(num):
	for x in range(num-2):
		if(num%(x+2)==0): return False
	return True

def endless(num):
	length=0
	list=[]
	for x in range(num-1):
		if(num%(x+2)==0 and prime(x+2)): list.append(x+2)
	for n in list:
		if(n!=2 and n!=5):
			return True
	return False
	#return length
	
d=1
top=6
biggest=1
for x in range(9):
	print(endless(x+2),x+2)
