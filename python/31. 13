/**/

def toStr(n,base):
   convertString="0123456789ABCDEF"
   if n < base:
      return convertString[n]
   else:
      return toStr(n//base,base) + convertString[n%base]

coins=[1,2,5,10,20,50,100,200]
ways=0
n=0
while(n<=200):
	n+=1
	total=0
	for num in (toStr(n,8)):
		total+=coins[int(num)]
	if(total==200):
		ways+=1
print(ways)
