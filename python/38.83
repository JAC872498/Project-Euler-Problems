"""
"""

def thing(num,n):
	wrd=""
	for n in range(n):
		wrd+=str(num*(n+1))
	if(len(wrd)>9): return -1
	if(len(wrd)<9): return 0
	for x in range(1,10):
		if(str(x) not in wrd): return 0
	return int(wrd)

num=9
n=4
topPan=918273645
while(num<=987654321):
	n+=1
	if(thing(num,n)>topPan):
		topPan=thing(num,n)
	elif(thing(num,n)==-1):
		if(n==2):
			break
		num+=1
		n=2
print(topPan)
