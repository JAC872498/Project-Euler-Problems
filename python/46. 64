"""

def isPrime(num):
	for x in range(2,num):
		if(num%x==0): return False
	return True

def conjecture(x,primes):
	for prime in primes:
		sqr=1
		while(prime+(2*(sqr*sqr))<=x):
			if(prime+(2*(sqr*sqr))==x): return True
			sqr+=1
	return False

primes=[3]
curr=9
"""for p in range(3,):
	if(isPrime): 
		primes.append(p)
		lastPrime=p"""
while(not isPrime(curr) and conjecture(curr,primes)):
	curr+=2
	if(lastPrime<curr):
		for p in range(lastPrime,curr+1):
			if(isPrime):
				primes.append(p)
				lastPrime=p
	if(not isPrime(curr) and not conjecture(curr,primes)):
			print(curr)
