from itertools import permutations

def isCube(n,cubes):
	if(n in cubes):
		return(True)
	c=int(n**(1/3.))
	return(c**3==n) or ((c+1)**3==n)

def findPerms(n,cubeNums):
	tot=0
	cubes=[]
	for perm in permutations(int(i) for i in str(n)):
		n=0
		for p in perm:
			n=(n*10)+p
		if(n not in cubes and isCube(n,cubeNums)):
			tot+=1
			cubes.append(n)
	return(tot)

cubeNums=[]
num=0
while(findPerms(num,cubeNums)!=5):
	cubeNums.append[num**3]
	num+=1
print(num)
