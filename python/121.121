def bouncy(n):
	if(n<100): return False
	inc=False
	dec=False
	for num in range(1,len(str(n))):
		if(int(str(n)[num-1])>int(str(n)[num])): dec=True
		elif(int(str(n)[num-1])<int(str(n)[num])): inc=True
	return(inc and dec)
perA=19602
perB=2178
num=perA+perB
while((perA/(perA+perB))<0.99):
	num+=1
	if(bouncy(num)):
		perA+=1
	else:
		perB+=1
print(num,(perA/(perA+perB)))
