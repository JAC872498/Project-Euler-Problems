def pan(num):
	for x in range(10):
		if(str(x) not in str(num)):
			return False
	return True
def subDiv(num,divs):
	for x in range(2,9):
		if(int(str(num)[x-1:x+2])%divs[x-2]!=0): return False
	return True
num=1406357289
divs=[2,3,5,7,11,13,17]
if(pan(num)):
	print(subDiv(num,divs))
