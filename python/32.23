def py(a,b,c,p):
	if(a+b+c!=p): return False
	return((a*a)+(b*b)==(c*c))

scores=[]
for p in range(1001):
	scores.append(0)
	for c in range(p):
		for b in range(c):
			for a in range(b+1):
				if(py(a,b,c,p)):
					scores[p]+=1
topN=0
top=0
for n in range(len(p)):
	if(p[n]>top):
		top=p[n]
		topN=n
print(topN)
