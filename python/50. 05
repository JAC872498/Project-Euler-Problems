def isPrime(num):
	if(num%2==0): return False
	for x in range(3,num,2):
		if(num%x==0): return False
	return True

def sectionTotal(length,spot,primes=primes):
	return 0#TODO: calculate the sum of 'length' number of items from 'primes' starting from 'spot'

primes=[2]
topLen=0
topNum=0
consecLen=1
spot=0
for n in range(3,999,2):
	if(isPrime(n)):
		primes.append(n)
print(primes)
while(True):
	if(len(str(sectionTotal()))>2 and spot==0):
		print(sectionTotal())
		break
	elif(len(str(sectionTotal()))>2):
		consecLen+=1
		spot=0
	if(consecLen>topLen and isPrime(sectionTotal()):
		topLen=consecLen
		topNum=sectionTotal()
	spot+=1
print("X",topNum,"X",topLen)
