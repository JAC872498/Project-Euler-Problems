primes=[2]
currLen=0
consecLen=1
spot=0
for n in range(3,999,2):
	if(isPrime(n)):
		primes.append(n)
print(primes)
while(True):
	if(len(str(sectionTotal()))>2 and spot==0):
		print(sectionTotal())
		break
	elif(len(str(sectionTotal()))>2):
		consecLen+=1
		spot=0
	elif(consecLen>currLen and isPrime(sectionTotal()):
		currLen=consecLen
