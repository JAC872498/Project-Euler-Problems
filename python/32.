def perf(n):#False==not abundant, True==abundant
	total=0
	for x in range(n/2):
		if(n%(x+1)==0):
			total+=x+1
			if(total>n):
				return True
	return False
	 
total=0
for m in range(28124):
	total+=(m+1)
	for n in range((m+1)/2):
		if(perf((m+1)-(n+1))==True and perf(n+1)==True):
			total-=(m+1)
			break
print(total)
