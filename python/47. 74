import math

def isPrime(num):
	for n in range(2,math.ceil(num/2)+1):
		if(num%n==0): return(False)
	return(True)

def divis(num,used=[]):#testing w/ 4 case
	if(isPrime(num) and len(used)==1):
		return(True)
	if(len(used)>=4 or num%1!=0):
		return(False)
	for n in range(2,num):
		used=[]
		if(isPrime(n)):
			if(n==num and len(used)==1):
				return(True)
			elif(num%n==0 and n not in used):
				used.append(n)
				return(divis(int(num/n),used))
			elif(num%n==0 and n in used):
				return(divis(int(num/n),used))
	return(False)
	"""if(len(used)>=4 or num%1!=0):
		return(False)
	for n in range(2,int(num)):
		used2=[]
		num2=0
		if(n==num and len(used)==3 and isPrime(n)):
			return(True)
		if(n not in used and isPrime(n) and num%n==0):
			used2.append(used)
			used2.append(n)
			num2=num/n
			return(divis(num2,used2))
		elif(n in used and isPrime(n) and num%n==0):
			num2=num/n
			return(divis(num2,used2))"""

