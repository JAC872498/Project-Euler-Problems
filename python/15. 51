"""
"""

def paths(sqrSize,currX,currY,tot):
	if(currX==sqrSize and currY==sqrSize):
		return tot
	elif(currX<sqrSize and currY<sqrSize):
		return paths(sqrSize,currX+1,currY,tot+1)+paths(sqrSize,currX,currY+1,tot+1)
	elif(currX<sqrSize):
		return paths(sqrSize,currX+1,currY,tot+1)
	else:
		return paths(sqrSize,currX,currY+1,tot+1)
print(paths(20,0,0,0))
