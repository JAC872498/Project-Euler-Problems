"""
"""

lastRow=[1,1]
currRow=[]
sqrLength=20
sqrCurr=2
for t in range(3,(sqrLength*2)+2):
	for n in range(len(lastRow)+1):
		if(n==0 or n==len(lastRow)):
			currRow.append(1)
		else:
			currRow.append(lastRow[n]+lastRow[n-1])
	#print(currRow)
	if(t%2!=0):
		print(sqrCurr-1,currRow[int(len(currRow)/2)])
		sqrCurr+=1
	lastRow=currRow
	currRow=[]
