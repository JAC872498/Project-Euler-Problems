"""
"""

lastRow=[1,1]
currRow=[]
sqrLength=6
for t in range(3,sqrLength+1):
	for n in range(len(lastRow)+1):
		if(n==0 or n==len(lastRow)):
			currRow.append(1)
		else:
			currRow.append(lastRow[n]+lastRow[n-1])
	print(currRow)
	if(t%2!=0):
		print(currRow[int(len(currRow)/2)])
	lastRow=currRow
	currRow=[]
