"""
"""

def palin(nu):
	num=str(nu)
	return(num==num[::-1])

def findPalin(num,t=0):
	if(palin(num) and t!=0):
		return(True)
	num+=int(str(num)[::-1])
	t+=1
	if(t>50):
		return(False)
	#print(num,t)
	return(findPalin(num,t))

print(palin(4994),findPalin(4994))
tot=0
for n in range(10000):
	#print(n)
	if(palin(n) and not findPalin(n)):
		tot+=1
print(tot)
