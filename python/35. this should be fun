"""
"""

def prime(i):
	for x in range(2,i):
		if(i%x==0):
			return False
	return True

def circ(num):
	wrd=str(num)
	for x in range(len(wrd)):
		currWrd=""
		currWrd=wrd[x:]+wrd[:x]
		#print(currWrd)
		if not(prime(int(currWrd))):
			return False
	return True

tot=0
for n in range(2,1000000):
	if(circ(n)):
		#print(n)
		tot+=1
print(tot)
