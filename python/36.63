def palindrome(num):
	wrd=str(num)
	if(len(wrd)%2==0):
		for x in range(int(len(wrd)/2)):
			if(wrd[x]!=wrd[-(x+1)]):
				return False
	else:
		for x in range(int(len(wrd)/2)+1):
			if(wrd[x]!=wrd[-(x+1)]):
				return False
	return True

sum=0
for x in range(1,1000000):
	if(palindrome(x) and palindrome("{0:b}".format(x))):
		sum+=x
print(sum)
