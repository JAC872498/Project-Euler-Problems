def palindrome(num):
	wrd=str(num)
	if(len(wrd)%2==0):
		for x in range(int(len(wrd)/2)):
			if(wrd[x]!=wrd[-(x+1)]):
				return False
	else:
		for x in range(int(len(wrd)/2)+1):
			if(wrd[x]!=wrd[-(x+1)]):
				return False
	return True
print(palindrome(585))
print(palindrome("{0:b}".format(585)))
