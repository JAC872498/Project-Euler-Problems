def leapYear(year):
    return(year%4==0 and ((year%100==0 and year%400==0)or(year%100!=0)))

month=[31,28,31,30,31,30,31,31,30,31,30,31]
leapMonth=[31,29,31,30,31,30,31,31,30,31,30,31]
count=0
day=0
for n in month:
    for x in range(n):
        day+=1
        if((day+1)%7==0):
            count+=1
print(count)
